AA,QAAA,CAAA,aAAA,OAAA,QAAA,YAAF;AAAA,KAAA;AAGC,GANT,EAIC,+BAJD,C,iCAcE,EALM,MAAA,CAAA,KAAA,CAAM,I,kBAFd,mBAAA,CAMS,QANT,EAMS;UAAA;AALP,IAAA,KAAK,EAAC,gCAKC;AAHN,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA;AAAA,aAAE,QAAA,CAAA,OAAA,IAAA,QAAA,CAAA,OAAA,OAAA,QAAA,YAAF;AAAA,KAAA;AAGC,GANT,EAIC,YAJD,C,iCAOE,CAfJ,CAqBI,E,CALQ,KAAA,CAAA,S,kBAAZ,mBAAA,CAGM,KAHN,EAGM,UAHN,EAGM,CAFiB,MAAA,CAAA,KAAA,CAAM,I,kBAA3B,YAAA,CAAmC,wBAAnC,EAAmC;AAAA,IAAA,GAAA,EAAA;AAAA,GAAnC,C,mBACA,YAAA,CAAe,eAAf,EAAe;AAAA,IAAA,GAAA,EAAA;AAAA,GAAf,C,CACI,CAHN,C,mBAIA,YAAA,CAAe,eAAf,EAAe;AAAA,IAAA,GAAA,EAAA;AAAA,GAAf,C,CACI,CAtBN,C","sourcesContent":["<template>\n  <TitleBar />\n  <div class=\"container mt-3\">\n    <p class=\"text-end border-bottom\">\n      <button\n        class=\"btn btn-secondary m-2 float-right\"\n        v-if=\"!store.user && !showLogin\"\n        @click=\"showLoginFunc\"\n      >\n        Login to Add/Manage listing\n      </button>\n      <button\n        class=\"btn btn-danger m-2 float-right\"\n        v-if=\"store.user\"\n        @click=\"signOut\"\n      >\n        Sign out\n      </button>\n    </p>\n    <div v-if=\"!showLogin\">\n      <UserDashboard v-if=\"store.user\" />\n      <Home v-else />\n    </div>\n    <Auth v-else />\n  </div>\n</template>\n\n<script>\nimport TitleBar from \"./components/TitleBar.vue\";\nimport { store } from \"./store\";\nimport { supabase } from \"./supabase\";\nimport UserDashboard from \"./components/UserDashboard.vue\";\nimport Home from \"./components/Home.vue\";\nimport Auth from \"./components/Auth.vue\";\nexport default {\n  name: \"App\",\n  data() {\n    return {\n      showLogin: false,\n    };\n  },\n  components: {\n    TitleBar,\n    Home,\n    UserDashboard,\n    Auth,\n  },\n  setup() {\n    \n     store.user = null;\n    supabase.auth.onAuthStateChange((_, session) => {\n      if (session && session.user) {\n        store.user = session.user;\n      } else {\n        store.user = null;\n      }\n    });\n\n    return {\n      store,\n    };\n  },\n  methods: {\n    showLoginFunc() {\n      this.showLogin = true;\n    },\n    async signOut() {\n      const { error } = await supabase.auth.signOut();\n      if (!error) {\n        console.log(\"Logged out successfully!\");\n      }\n    },\n  },\n};\n</script>\n\n<style>\n\n.action-btn {\n  background-color: rgb(36, 180, 126);\n  color : #fff;\n}\n\n</style>\n"],"sourceRoot":""}]}