nts: {\n    TitleBar,\n    Home,\n    UserDashboard,\n    Auth,\n  },\n  setup() {\n    \n     store.user = null;\n    supabase.auth.onAuthStateChange((_, session) => {\n      if (session && session.user) {\n        store.user = session.user;\n      } else {\n        store.user = null;\n      }\n    });\n\n    return {\n      store,\n    };\n  },\n  methods: {\n    showLoginFunc() {\n      this.showLogin = true;\n    },\n    async signOut() {\n      const { error } = await supabase.auth.signOut();\n      if (!error) {\n        console.log(\"Logged out successfully!\");\n      }\n    },\n  },\n};\n</script>\n\n<style>\n#app {\n}\n</style>\n"]}]}