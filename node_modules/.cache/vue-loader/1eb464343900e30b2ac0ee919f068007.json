LocationCard,\n  },\n  data() {\n    return {\n      navigatorError: false,\n      coords: {},\n      items: [],\n      radii: 20,\n    };\n  },\n  created() {\n    if (navigator.geolocation) {\n      navigator.geolocation.getCurrentPosition(this.showPosition);\n    } else {\n      console.log(\"Geolocation is not supported by this browser.\");\n    }\n  },\n\n  methods: {\n    radiusChange() {\n      console.log(this.radii);\n      this.getLocations(this.coords, this.radii);\n    },\n    showPosition(position) {\n      this.coords = position.coords;\n      this.getLocations(this.coords, 100);\n    },\n    async getLocations(position, radius = 100) {\n      console.log(position, radius);\n      const { data: todos, error } = await supabase.rpc(\"get_all_locations\", {\n        radius: radius,\n        point:\n          \"SRID=4326;POINT(\" +\n          position.latitude +\n          \" \" +\n          position.longitude +\n          \")\",\n      });\n      if (error) {\n        console.log(\"error\", error);\n        return;\n      }\n      // handle for when no todos are returned\n      if (todos === null) {\n        allTodos.value = [];\n        return;\n      }\n      this.items = todos;\n      console.log(this.items);\n    },\n  },\n};\n</script>\n\n<style scoped>\n</style>\n"]}]}