eListing(id) {\n      console.log(id);\n      const { error } = await supabase\n        .from(\"all_locations\")\n        .delete()\n        .match({ id: id });\n\n      // console.log(data, error);\n      if (!error) {\n         this.$parent.getUserListings(store.user.id);\n      }\n    },\n  },\n};\n</script>\n\n<style scoped>\n.directions-btn {\n  background-color: rgb(36, 180, 126);\n}\n</style>\n"]}]}